        </activity>
        <activity android:name=".activity.WelcomeActivity"></activity>
        <activity android:name=".activity.Inscrir"></activity>
        <activity android:name=".activity.Seconnecte"></activity>
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.BookActivity"></activity>
